#!yasl_linux

{
	%@ (
		0:=:1
		1:%!= ? (
			0:=:2
			2:#:2
			2:2
			2:== ? 2:+:2 : ()
			0:!
		) : (
			0:!
			2:#:2
			2:? (
				"\n":2
				2:+:2
				2:print
			)
		)
		1:!
	)
	2:#:2
	2:? (
		"\n":2
		2:+:2
		2:print
	)
}	split

! !

#
2
!= ? ("Wrong number of arguments\n" print 1 exit)

[#]
1
!= ? ("Delimiter is incorrect\n" print 1 exit)

0:=:1
!
0
2 ^
][:3
3:!
3:#:3
3:#(
	3:=:0
	3:!
)
split
